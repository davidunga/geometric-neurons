            "exclude_reflected": true,
            "max_samples": 200000,
            "random_neural": false,
            "balance": true
        },
        "thresh_prcntl": {
            "low": 10,
            "high": 20
        },
        "train": {
            "epochs": 100,
            "rand_seed": 0,
            "batch_size": 32,
            "pepper_sigma": 0.5,
            "optimizer_params": {
                "lr": 5e-05
            },
            "loss_margin": 1
        }
