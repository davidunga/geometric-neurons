{
        "target_ftr": "TrjAffine_dists",
        "model": {
            "descriptor_size": 5,
            "dropout": 0.5
        },
        "data": {
            "matf": "NichoData_Segments_RS_200ms.mat",
            "ctrl_kind": "rel",
            "equalize_ctrls": true,
            "ctrls_as_neural": false,
            "normalize_neural": true,
            "p_train": 0.8,
            "p_val": 0.1,
            "neural_filt_sz": 3,
            "rand_seed": 0,
            "scale_range": {
                "min": 0.2,
                "max": 1
